    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
    "protractor": "^7.0.0",
    "selenium-webdriver": "^4.0.0-beta.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.js'"
